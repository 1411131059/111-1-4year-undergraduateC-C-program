#include<iostream>
using namespace std;

void cube(int&);
float cylinder(int&, int&);
int volume(int&, int&, int&);

int main()
{
	int x, y, z;
	double pi = 3.14159;
	
	cout << "\nEnter the side length of cube: ";
	cin >> x;
	cube(x);
	cout << "Volume of cube: " << x << endl;

	cout << "\nEnter the height of cylinder: ";
	cin >> y;
	cout << "\nEnter the radius of cylinder: ";
	cin >> z;
	cout << "Volume of cylinder: " << cylinder(y, z) << endl;
	
	cout << "\nEnter the length of cuboid: ";
	cin >> x;
	cout << "\nEnter the width of cuboid: ";
	cin >> y;
	cout << "\nEnter the height of cuboid: ";
	cin >> z;
	cout << "Volume of cuboid: " << volume(x, y, z) << endl;
}

void cube(int &num)
{
	num = num * num * num;
}

float cylinder(int& height, int& radius)
{
	double pi = 3.14159;
	return radius * radius * pi * height;
}

int volume(int& length, int& width, int& height)
{
	return length * width * height;
}
